{
  "AwsSecretsManagerConfiguration:Enabled": true,
  "AllowedHosts": "*",
  "ExternalProvidersConfiguration": {
    "UseGoogleProvider": false,
    "GoogleClientId": "{GoogleClientId}",
    "GoogleClientSecret": "{GoogleClientSecret}",
    "GoogleCallbackPath": "",
    "UseGitHubProvider": false,
    "GitHubClientId": "{GitHubClientId}",
    "GitHubClientSecret": "{GitHubClientSecret}",
    "GitHubCallbackPath": "",
    "UseDiscordProvider": false,
    "DiscordClientId": "{DiscordClientId}",
    "DiscordClientSecret": "{DiscordClientSecret}",
    "DiscordCallbackPath": "",
    "UseRedditProvider": false,
    "RedditClientId": "{RedditClientId}",
    "RedditClientSecret": "{RedditClientSecret}",
    "RedditCallbackPath": "",
    "UseAmazonProvider": false,
    "AmazonClientId": "{AmazonClientId}",
    "AmazonClientSecret": "{AmazonClientSecret}",
    "AmazonCallbackPath": "",
    "UseTwitchProvider": false,
    "TwitchClientId": "{TwitchClientId}",
    "TwitchClientSecret": "{TwitchClientSecret}",
    "TwitchCallbackPath": "",
    "UsePatreonProvider": false,
    "PatreonClientId": "{PatreonClientId}",
    "PatreonClientSecret": "{PatreonClientSecret}",
    "PatreonCallbackPath": "",
    "UseWordpressProvider": false,
    "WordpressClientId": "{WordpressClientId}",
    "WordpressClientSecret": "{WordpressClientSecret}",
    "WordpressCallbackPath": ""
  },
  "AccountConfiguration": {
    "RegisterConfiguration": {
      "Enabled": true
    },
    "LoginConfiguration": {
      "LoginPolicy": "All"
    },
    "ProfilePictureConfiguration": {
      "Enabled": true,
      "AutoGenerate": true,
      "ProfilePictureUploadType": "File",
      "AllowedFileExtensions": [".jpg", ".jpeg", ".png", ".gif"],
      "DefaultUrl": "https://avatars.dicebear.com/api/initials/",
      "BaseUrl": "https://localhost:7003/"
    }
  },
  "CspTrustedDomains": [
    "ws://localhost:57172"
  ],
  "IdentityOptions": {
    "Password": {
      "RequiredLength": 8
    },
    "User": {
      "RequireUniqueEmail": true
    },
    "SignIn": {
      "RequireConfirmedAccount": true,
      "RequireConfirmedEmail": true,
      "RequireConfirmedPhoneNumber": true
    },
    "Lockout": {
      "AllowedForNewUsers": true,
      "LockoutDuration": 60,
      "MaxFailedAccessAttempts": 3
    }
  },
  "IdentityServerOptions": {
    "KeyManagement": {
      "Enabled": true
    },
    "Events": {
      "RaiseErrorEvents": true,
      "RaiseInformationEvents": true,
      "RaiseFailureEvents": true,
      "RaiseSuccessEvents": true
    },
    "UserInteraction": {
      "LoginUrl": "https://localhost:3000/auth/login",
      "LogoutUrl": "https://localhost:3000/auth/logout",
      "ErrorUrl": "https://localhost:3000/error",
      "ConsentUrl": "https://localhost:3000/app/consent",
      "AllowOriginInReturnUrl": true
    }
  }
}
