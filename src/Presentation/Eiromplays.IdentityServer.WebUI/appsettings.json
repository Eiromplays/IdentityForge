{
  "Logging": {
      "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
      }
    },
  "AwsSecretsManagerConfiguration:Enabled": false,
  "AllowedHosts": "*",
  "DatabaseConfiguration": {
    "DatabaseProvider": "PostgreSql",
