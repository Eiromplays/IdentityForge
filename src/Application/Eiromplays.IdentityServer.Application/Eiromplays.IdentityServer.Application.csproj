    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Domain\Eiromplays.IdentityServer.Domain\Eiromplays.IdentityServer.Domain.csproj" />
  </ItemGroup>

</Project>
